generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Email {
  id        Int      @id @default(autoincrement())
  subject   String
  body      String
  sentAt    DateTime @default(now())
  sentTo    String
  sentFrom  String
  sentReply String?
  
  @@map("emails")
}